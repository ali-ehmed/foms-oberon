<div class="page-header">
	<h3>Tabular <small>Report</small></h3>
</div>
<div class="row">
	<div class="col-md-12 col-sm-12 col-lg-12 col-xs-12">
    <div class="table-responsive">
  		<table  class="table table-hover table-striped">
        <thead>
          <tr>
            <th><%= column_name %></th>
            <th>Revenue</th>
            <th>Expense</th>
            <th>Profit</th>
            <th>Loss</th>
          </tr>
        </thead>
        <tbody>
          <% if tabular_data.each do |tab_data| %>
          	<tr>
          		<td><%= tab_data[0] %></td>
          		<% tab_data[1].each do |report| %>
        				<td><%= report[:y] %></td>
          		<% end %>
          	</tr>
          <% end.empty? %>
          	<tr>
          		<td>No Data</td>
          		<td>No Data</td>
          		<td>No Data</td>
          		<td>No Data</td>
          		<td>No Data</td>
        		</tr>
          <% end %>
      	</tbody>
    	</table>
    </div>
	</div>
</div>